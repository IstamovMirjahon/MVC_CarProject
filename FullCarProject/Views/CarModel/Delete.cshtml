@model Entity.Models.CarModels.CarModel

@{
    ViewData["Title"] = "Delete Car Model";
}


<div class="mt-4">
    <h2>Delete Car Model</h2>
    <h5>Are you sure you want to delete the company <strong>@Model.Model</strong>?</h5>
    <form asp-action="Delete" method="post">
        <input type="hidden" asp-for="Id" />
        <div class="form-group mt-3">
            <button type="submit" class="btn btn-danger">Saqlash</button>
        </div>
    </form>
</div>


